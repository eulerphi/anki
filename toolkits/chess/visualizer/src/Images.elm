module Images exposing (..)


arrowsUri : String
arrowsUri =
    "url(data:image/svg+xml;base64,)"


boardUri : String -> String
boardUri name =
    case name of
        "blue" ->
            "url('data:image/svg+xml;base64,')"

        "green" ->
            "url(data:image/svg+xml;base64,)"

        -- gray
        _ ->
            "url(data:image/svg+xml;base64,)"


